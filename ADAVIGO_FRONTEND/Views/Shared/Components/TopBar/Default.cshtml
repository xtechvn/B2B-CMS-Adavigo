    <h2 class="title-page"></h2>
    <a href="#" class="phone">
        <svg class="icon-svg">
            <use xlink:href="/images/icons/icon.svg#phone"></use>
        </svg>
        <span>0936 191 192</span>
    </a>
    <div class="my-adavigo">
       
        <button type="submit" title="Tìm kiếm" class="btn-search">
            <svg class="icon-svg">
                <use xlink:href="/images/icons/icon.svg#search"></use>
            </svg>
        </button>
        <div class="notifi">
            <a href="">
                <svg class="icon-svg">
                    <use xlink:href="/images/icons/icon.svg#notifi"></use>
                </svg>
            </a>
        </div>
        <div class="my-user dropdown show">
            <a class="login dropdown-toggle" href="#" role="button" id="dropdownMenuLink" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                <svg class="icon-svg">
                    <use xlink:href="/images/icons/icon.svg#user"></use>
                </svg>
                <span class="nav_username">@(User.Identity.Name == null ? "" : User.Identity.Name.ToString())</span>
            </a>
            <div class="dropdown-menu dropdown-menu-right" aria-labelledby="dropdownMenuLink">
                <a class="dropdown-item" href="/account" style="color:#00264D">
                    <svg class="icon-svg">
                        <use xlink:href="/images/icons/icon.svg#user"></use>
                    </svg> <span>Thông tin tài khoản</span>
                </a>
                @if (ViewBag.ParentId != null)
                {
                    if (int.Parse(ViewBag.ParentId.Value) <= 0)
                    {
                        <a class="dropdown-item" href="/account/listAccount" style="color:#00264D">
                            <svg class="icon-svg">
                                <use xlink:href="/images/icons/icon.svg#user"></use>
                            </svg> <span>Quản lý tài khoản</span>
                        </a>
                    }

                }
                else
                {
                    <script>
                        window.location.href = `/login`
                    </script>
                }
                <a class="dropdown-item" href="/booking" style="color:#00264D">
                    <svg class="icon-svg">
                        <use xlink:href="/images/icons/icon.svg#booking"></use>
                    </svg> <span>Quản lý đơn khách sạn</span>
                </a>
                <a class="dropdown-item" href="/tour/GetListTour" style="color:#00264D">
                    <svg class="icon-svg">
                        <use xlink:href="/images/icons/icon.svg#booking"></use>
                    </svg> <span>Quản lý đơn Tour</span>
                </a>
                <a class="dropdown-item" href="/booking/RequestHotelBooking" style="color:#00264D">
                    <svg class="icon-svg">
                        <use xlink:href="/images/icons/icon.svg#booking"></use>
                    </svg> <span>Kiểm tra quỹ phòng</span>
                </a>
                <a class="dropdown-item" href="/fund/history" style="color:#00264D">
                    <svg class="icon-svg">
                        <use xlink:href="/images/icons/icon.svg#help"></use>
                    </svg> <span>Quản lý quỹ</span>
                </a>
                <a class="dropdown-item" onclick="_global.Logout()" style="color:#00264D">
                    <svg class="icon-svg">
                        <use xlink:href="/images/icons/icon.svg#Out"></use>
                    </svg> <span>Đăng xuất</span>
                </a>
            </div>
        </div>
